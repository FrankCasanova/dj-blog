{
    "builds": [
      {
        "src": "dj_blog/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "dj_blog/wsgi.py"
      }
    ]
  }